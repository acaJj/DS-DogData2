<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/inputFragment">
    <fragment
        android:id="@+id/inputFragment"
        android:name="sheridan.jaca.dogdata2.InputFragment"
        android:label="InputFragment" >
        <action
            android:id="@+id/action_input_to_output"
            app:destination="@id/outputFragment" />
    </fragment>
    <fragment
        android:id="@+id/outputFragment"
        android:name="sheridan.jaca.dogdata2.OutputFragment"
        android:label="OutputFragment" >
        <action
            android:id="@+id/action_output_to_input"
            app:destination="@id/inputFragment" />
    </fragment>
</navigation>